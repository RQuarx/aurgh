<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="3.0"/>
  <object class="GtkBox" id="aur_tab">

    <property name="halign">fill</property>
    <child>

      <object class="GtkScrolledWindow" id="scroll_window">

        <property name="hscrollbar-policy">never</property>
        <property name="overlay-scrolling">false</property>
        <property name="kinetic-scrolling">true</property>
        <property name="halign">fill</property>
        <child>

          <object class="GtkBox">

            <property name="orientation">vertical</property>
            <property name="name">tab-content-container</property>
            <child>

              <object class="GtkBox" id="content">

                <property name="orientation">vertical</property>
                <property name="name">tab-content</property>
                <property name="hexpand">true</property>
                <property name="halign">fill</property>
                <property name="spacing">5</property>

              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>